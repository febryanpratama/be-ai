    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.196",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.4.6",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "eslint": "^9.10.0",
